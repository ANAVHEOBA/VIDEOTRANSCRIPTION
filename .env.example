# Bluesky Client Configuration
BLUESKY_CLIENT_ID=https://your-app.com/oauth/client-metadata.json
BLUESKY_CLIENT_TYPE=web
BLUESKY_CLIENT_NAME="Your App Name"
BLUESKY_CLIENT_URI=https://your-app.com
BLUESKY_REDIRECT_URI=https://your-app.com/auth/bluesky/callback
BLUESKY_LOGO_URI=https://your-app.com/logo.png

# Token Configuration
BLUESKY_TOKEN_EXPIRES=3600
BLUESKY_REFRESH_BUFFER=300
BLUESKY_MAX_REFRESH_ATTEMPTS=3

# API Configuration
BLUESKY_API_URL=https://bsky.social

# Media Configuration
BLUESKY_MEDIA_MAX_SIZE=10485760

# Rate Limiting
BLUESKY_POSTS_PER_HOUR=50
BLUESKY_UPLOADS_PER_HOUR=100
BLUESKY_REQUEST_DELAY=100

# Security
BLUESKY_VERIFY_SSL=true
BLUESKY_REQUEST_TIMEOUT=30
BLUESKY_DEBUG=false

# Cache Configuration
BLUESKY_CACHE_ENABLED=true
BLUESKY_CACHE_DURATION=3600
BLUESKY_CACHE_STORE=redis

# Logging Configuration
BLUESKY_LOGGING_ENABLED=true
BLUESKY_LOG_CHANNEL=bluesky